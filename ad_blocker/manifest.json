{
    "name" : "Ad Blocker",
    "version" : "0.0.1",
    "manifest_version" : 3,
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "logo.png"
    },
    "icons":{
        "128":"logo.png"
    },
    "permissions": ["webRequest","webRequestBlocking", "<all_urls>"],
    "background":{
        "scripts":["background.js"]
    }
    
}